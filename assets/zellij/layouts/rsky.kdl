layout {
    default_tab_template {
        pane size=1 borderless=true {
            plugin location="compact-bar"
        }
        children
    }

    cwd "~/utkarsh/solo/rsky"

    tab name="htop" {
      pane command="htop"
    }

    tab name="bmon" {
      pane command="bmon"
    }

    tab name="indigo" cwd="~/utkarsh/solo/indigo"

    tab name="rsky" focus=true

    tab name="relay"

    tab name="cmd"
}
